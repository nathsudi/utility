# Common environment configuration for utility scripts
# Edit these values with your actual configuration
# Usage: source scripts/config.env && ./scripts/bash/<script-name>.sh

# Cluster configuration
export CLUSTER_FQDN="your-cluster.domain.com"
export PROJECT_NAME="your-project"
export CLUSTER_NAME="your-cluster"

# Authentication
export USERNAME="your-username"
export PASSWORD="your-password"
